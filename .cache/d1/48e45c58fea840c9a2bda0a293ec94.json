hy START */\r\n@media (min-width: 700px) {\r\n  :root {\r\n    --fs-h1: 2.8rem;\r\n    --fs-body: 1.3rem;\r\n  }\r\n}\r\n\r\nh1,\r\np {\r\n  margin: 0;\r\n}\r\n\r\nh1 {\r\n  line-height: 1.2;\r\n  font-size: var(--fs-h1);\r\n  font-family: var(--primary-font);\r\n}\r\n/* Typography END */\r\n\r\n/* Main Section and Grid Container*/\r\nmain {\r\n  min-height: 100vh;\r\n}\r\n\r\n.grid {\r\n  height: 100%;\r\n  display: grid;\r\n  grid-gap: 2rem 0;\r\n  grid-template-rows: minmax(150px, 25vh) minmax(min-content, auto) minmax(\r\n      200px,\r\n      1fr\r\n    );\r\n  place-items: center;\r\n}\r\n\r\n/* Paragraph to Type Section Start */\r\n.content {\r\n  box-shadow: 0 0 15px rgba(122, 122, 122, 0.363);\r\n  width: 100%;\r\n  padding: 1rem;\r\n  border-radius: 0.5em;\r\n}\r\n\r\n.currentLine {\r\n  max-height: 5rem;\r\n  overflow: hidden;\r\n}\r\n\r\n.words {\r\n  --transformTop: 0;\r\n  user-select: none;\r\n  letter-spacing: 1.2px;\r\n  font-size: 1.35rem;\r\n  transition: 0.35s ease-in-out;\r\n  line-height: 2.5rem;\r\n  transform: translateY(var(--transformTop));\r\n}\r\n\r\n.content span {\r\n  display: inline-flex;\r\n  padding: 0.07rem 0.3rem;\r\n}\r\n\r\n/* Controls and Others */\r\n.controls {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  align-self: start;\r\n  gap: 1rem;\r\n  width: 100%;\r\n}\r\n\r\n.input-field {\r\n  flex-grow: 1;\r\n  max-width: 500px;\r\n  background-color: transparent;\r\n  outline: none;\r\n  border-radius: 0.5em;\r\n  font-size: 1.2rem;\r\n  padding: 0.4em 0.6em;\r\n  color: inherit;\r\n  letter-spacing: 1.3px;\r\n}\r\n\r\n.input-field:focus {\r\n  border: 4px ridge royalblue;\r\n  box-shadow: 0 0 5px rgba(122, 122, 122, 0.363);\r\n}\r\n.timer-container {\r\n  border-radius: 0.5em;\r\n  display: flex;\r\n  align-items: center;\r\n  letter-spacing: 1.35px;\r\n  font-weight: 600;\r\n  padding: 0.5em 0.9em;\r\n  background-color: rgb(92, 92, 92);\r\n}\r\n\r\n.timer {\r\n  font-size: 1.4rem;\r\n  letter-spacing: 1.3px;\r\n}\r\n\r\n.refresh {\r\n  margin: 0;\r\n  border: none;\r\n  cursor: pointer;\r\n  color: inherit;\r\n  font-size: 2rem;\r\n  border-radius: 0.5em;\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 0.5rem 0.9rem;\r\n  background-color: rgb(92, 92, 92);\r\n  transition: 0.35s ease-in-out;\r\n}\r\n\r\n.refresh:focus,\r\n.refresh:hover {\r\n  opacity: 0.8;\r\n  transform: translateX(1.1);\r\n}\r\n\r\n/* Results Table Section */\r\n.results-list-main {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 2em;\r\n}\r\n\r\n.results-list {\r\n  list-style-type: none;\r\n  padding: 0;\r\n  margin: 0;\r\n}\r\n\r\n.results-list li {\r\n  min-width: 265px;\r\n  font-family: \"Poppins\", var(--secondary-font);\r\n  display: flex;\r\n  gap: 2.5rem;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  margin: 0.3em 0;\r\n  padding: 0.35rem 0.7em;\r\n}\r\n\r\n.results-list li:nth-child(odd) {\r\n  background-color: rgb(92, 92, 92);\r\n}\r\n\r\n.results-list li span:last-child {\r\n  font-weight: 500;\r\n}\r\n\r\n/* Utility CLASSES START */\r\n.correct {\r\n  color: lightgreen;\r\n}\r\n\r\n.incorrect {\r\n  color: lightcoral;\r\n}\r\n\r\n.highlight {\r\n  background-color: lightslategrey;\r\n  border-radius: 0.2em;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"a4a33d78805a81f23c159f5912b440f1","cacheData":{"env":{}}}